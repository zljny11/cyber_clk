{
    "manifest_version": 3,
    "name": "Cyberpunk Focus Sidepanel",
    "version": "1.0",
    "description": "Wake the f**k up, Samurai. We have a task to burn.",
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "sidePanel"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "action": {
        "default_title": "Open Focus Clock"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    }
}